{
  "name": "PPUPIN",
  "description": "Prevention of Potentially Unwanted Personal Information Negligence",
  "version": "1.0",
  "manifest_version": 2,

  "browser_action": {
    "default_title": "PPUPIN",
    "default_popup": "popup.html"
  },
  "icons": {
    "16": "icon.png",
    "48": "icon.png",
    "128": "icon.png"
  },
  "permissions":[
    "activeTab",
    "storage"
  ]
}